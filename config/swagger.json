{
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"UserRole": {
				"enum": [
					"Admin",
					"ScoreEditor"
				],
				"type": "string"
			},
			"User": {
				"properties": {
					"name": {
						"type": "string"
					},
					"username": {
						"type": "string"
					},
					"password": {
						"type": "string"
					},
					"role": {
						"$ref": "#/components/schemas/UserRole"
					}
				},
				"required": [
					"name",
					"username",
					"password",
					"role"
				],
				"type": "object",
				"additionalProperties": false
			},
			"DeleteUserRequest": {
				"properties": {
					"username": {
						"type": "string"
					}
				},
				"required": [
					"username"
				],
				"type": "object",
				"additionalProperties": false
			},
			"AccessTokenTypes": {
				"enum": [
					"Basic",
					"Bearer"
				],
				"type": "string"
			},
			"LoginWithUsernameAndPasswordRequest": {
				"properties": {
					"username": {
						"type": "string"
					},
					"password": {
						"type": "string"
					}
				},
				"required": [
					"username",
					"password"
				],
				"type": "object",
				"additionalProperties": false
			},
			"Token": {
				"type": "string"
			},
			"AccessTokenRequest": {
				"properties": {
					"refreshToken": {
						"$ref": "#/components/schemas/Token"
					}
				},
				"required": [
					"refreshToken"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {}
	},
	"info": {
		"title": "inter-iit-scoreboard-server",
		"version": "1.0.0",
		"description": "This is the server for the scoreboard for the Inter-IIT-sports-meet made by Karan Gandhi",
		"license": {
			"name": "MIT"
		},
		"contact": {
			"name": "Karan-Gandhi"
		}
	},
	"openapi": "3.0.0",
	"paths": {
		"/admin/users": {
			"get": {
				"operationId": "GetUsers",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"items": {
										"$ref": "#/components/schemas/User"
									},
									"type": "array"
								}
							}
						}
					}
				},
				"tags": [
					"Authentication",
					"Admin"
				],
				"security": [],
				"parameters": []
			}
		},
		"/admin/user": {
			"delete": {
				"operationId": "DeleteUser",
				"responses": {
					"204": {
						"description": "No content"
					}
				},
				"tags": [
					"Authentication",
					"Admin"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/DeleteUserRequest"
							}
						}
					}
				}
			}
		},
		"/auth/loginWithUsernameAndPassword": {
			"post": {
				"operationId": "LoginWithUsernameAndPassword",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"properties": {
										"user": {
											"properties": {
												"role": {
													"$ref": "#/components/schemas/UserRole"
												},
												"name": {
													"type": "string"
												}
											},
											"required": [
												"role",
												"name"
											],
											"type": "object"
										},
										"accessToken": {
											"type": "string"
										},
										"type": {
											"$ref": "#/components/schemas/AccessTokenTypes"
										},
										"refreshToken": {
											"type": "string"
										}
									},
									"required": [
										"user",
										"accessToken",
										"type",
										"refreshToken"
									],
									"type": "object"
								}
							}
						}
					},
					"403": {
						"description": "Invalid username or password"
					}
				},
				"description": "Logs in a user with their username and password.",
				"tags": [
					"Authentication"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/LoginWithUsernameAndPasswordRequest"
							}
						}
					}
				}
			}
		},
		"/auth/accessToken": {
			"post": {
				"operationId": "AccessToken",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"properties": {
										"accessToken": {
											"type": "string"
										},
										"type": {
											"$ref": "#/components/schemas/AccessTokenTypes"
										},
										"refreshToken": {
											"type": "string"
										}
									},
									"required": [
										"accessToken",
										"type",
										"refreshToken"
									],
									"type": "object"
								}
							}
						}
					},
					"403": {
						"description": "Invalid Refresh token"
					}
				},
				"description": "Generates a new access and refresh token using the provided refresh token.",
				"tags": [
					"Authentication"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/AccessTokenRequest"
							}
						}
					}
				}
			}
		},
		"/auth/logout": {
			"delete": {
				"operationId": "Logout",
				"responses": {
					"204": {
						"description": "No content"
					}
				},
				"description": "Logs out the user by revoking the provided refresh token.",
				"tags": [
					"Authentication"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/AccessTokenRequest"
							}
						}
					}
				}
			}
		}
	},
	"servers": [
		{
			"url": "/"
		}
	]
}