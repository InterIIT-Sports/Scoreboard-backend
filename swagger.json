{
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"AccessTokenTypes": {
				"enum": [
					"Basic",
					"Bearer"
				],
				"type": "string"
			},
			"UserRole": {
				"enum": [
					"Admin",
					"ScoreEditor"
				],
				"type": "string"
			}
		},
		"securitySchemes": {}
	},
	"info": {
		"title": "inter-iit-scoreboard-server",
		"version": "1.0.0",
		"description": "This is the server for the scoreboard for the Inter-IIT-sports-meet",
		"license": {
			"name": "MIT"
		},
		"contact": {
			"name": "Karan-Gandhi"
		}
	},
	"openapi": "3.0.0",
	"paths": {
		"/auth/loginWithUsernameAndPassword": {
			"post": {
				"operationId": "LoginWithUsernameAndPassword",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"properties": {
										"user": {
											"properties": {
												"role": {
													"$ref": "#/components/schemas/UserRole"
												},
												"name": {
													"type": "string"
												}
											},
											"required": [
												"role",
												"name"
											],
											"type": "object"
										},
										"accessToken": {
											"type": "string"
										},
										"type": {
											"$ref": "#/components/schemas/AccessTokenTypes"
										},
										"refreshToken": {
											"type": "string"
										}
									},
									"required": [
										"user",
										"accessToken",
										"type",
										"refreshToken"
									],
									"type": "object"
								}
							}
						}
					}
				},
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"properties": {
									"password": {
										"type": "string"
									},
									"username": {
										"type": "string"
									}
								},
								"required": [
									"password",
									"username"
								],
								"type": "object"
							}
						}
					}
				}
			}
		}
	},
	"servers": [
		{
			"url": "/"
		}
	]
}